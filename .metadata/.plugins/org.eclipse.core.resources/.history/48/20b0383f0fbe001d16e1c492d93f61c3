public class Conta {
	double saldo;
	int agencia;
	int numeroConta;
	String titular;
	
	public void deposita(double valor){
		this.saldo = this.saldo + valor;
		System.out.println("Deposito de " + valor + "R$ realizado com sucesso");
	}
	
	public boolean saca(double valor){
		if(this.saldo >= valor) {
			this.saldo = this.saldo - valor;
			System.out.println("Saque de " + valor + "R$ realizado com sucesso");
			return true;
		} else {
			return false;
		}
	}
}